{
    "code-runner.runInTerminal": true,
    "code-runner.fileDirectoryAsCwd": false,
    "code-runner.executorMap": {
        "python": "cd ${workspaceFolder}/../worker_plan && PYTHONPATH=${workspaceFolder}/../worker_plan python3 -m pytest $fullFileName"
    },
    "code-runner.executorMapByFileExtension": {
        ".py": "cd ${workspaceFolder}/../worker_plan && PYTHONPATH=${workspaceFolder}/../worker_plan python3 -m pytest $fullFileName"
    }
}
